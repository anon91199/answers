/* 
 *  class Node {
 *    int data;
 *    Node left; 
 *    Node right;
 *    public Node() {
 *      data = 0;
 *    }
 *    public Node(int d)  {
 *      data = d;
 *    }
 *  }
 *
 *  The above class defines a tree node.
 */
class Result {
  static int treeHeight(Node root) {
    // Write your code here
          if(root==null) return -1;
      int left = treeHeight(root.left);
      int right = treeHeight(root.right);
      return Math.max(left,right)+1;
  }
}
